{
  "name": "@oniryk/dreamer-csv",
  "version": "0.0.1",
  "description": "delivery lucid queries as csv",
  "main": "build/index.js",
  "type": "module",
  "types": "build/index.d.ts",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "del-cli build",
    "test": "tsx --test ./tests/**/*.spec.*",
    "pni": "npm run build && npm pack && rm -rf ../demo/node_modules/@oniryk/dreamer-csv && mkdir -p ../demo/node_modules/@oniryk/dreamer-csv && tar -xzf *.tgz -C ../demo/node_modules/@oniryk/dreamer-csv --strip-components 1 && rm *.tgz"
  },
  "author": {
    "name": "brunoziie",
    "email": "eu@brunoziie.com"
  },
  "license": "MIT",
  "dependencies": {
    "papaparse": "^5.4.1"
  },
  "devDependencies": {
    "@adonisjs/core": "^6.14.1",
    "@adonisjs/eslint-config": "^1.3.0",
    "@adonisjs/prettier-config": "^1.4.0",
    "@adonisjs/tsconfig": "^1.4.0",
    "@types/node": "^22.7.5",
    "@types/papaparse": "^5.3.14",
    "del-cli": "^6.0.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "@adonisjs/core": "^6.14.1"
  }
}
